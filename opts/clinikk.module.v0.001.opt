						<term_definitions code="at0000">
							<items id="text">Capillary refill time (CRT)</items>
							<items id="description">Measurement of the time taken for color to return to an external capillary bed after pressure is applied to cause blanching.</items>
							<items id="comment"> Sometimes expressed as 'capillary return'.</items>
						</term_definitions>
						<term_definitions code="at0001">
							<items id="text">History</items>
